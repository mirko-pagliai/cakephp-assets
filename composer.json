{
    "name": "mirko-pagliai/cakephp-assets",
    "description": "Assets plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/cakephp-assets",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.x-dev",
        "matthiasmullie/minify": "^1.3",
        "mirko-pagliai/php-tools": "dev-php7"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-next",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/test_app/TestApp/",
            "Assets\\Test\\": "tests",
            "TestPlugin\\": "tests/test_app/Plugin/TestPlugin/src"
        }
    }
}
