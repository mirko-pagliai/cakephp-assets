{
    "name": "mirko-pagliai/cakephp-assets",
    "description": "Assets plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/cakephp-assets",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9 <7.3",
        "cakephp/cakephp": "^3.4.13",
        "matthiasmullie/minify": "^1.3",
        "mirko-pagliai/php-tools": "dev-php5.6"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0|^4.0",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/test_app/TestApp/",
            "Assets\\Test\\": "tests"
        }
    }
}
